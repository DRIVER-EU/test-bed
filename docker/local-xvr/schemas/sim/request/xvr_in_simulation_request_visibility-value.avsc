{
  "name": "RequestVisibility",
  "namespace": "eu.driver.model.sim.request",
  "doc": "A visibility request is a specific request for changing the visibility status of a given simulation entity.",
  "type": "record",
  "fields": [
    {
      "name": "id",
      "doc": "Unique case-insensitive identifier of the request",
      "type": "string"
    },
    {
      "name": "applicant",
      "doc": "Unique case-insensitive identifier of the connected application sending the request",
      "type": "string"
    },
    {
      "name": "entity",
      "doc": "Unique case-insensitive identifier of the targeted entity",
      "type": "string"
    },
    {
      "name": "visible",
      "doc": "Boolean value directing the visibility state of the targeted entity",
      "type": "boolean"
    }
  ]
}
